    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.25",
    "jquery": "3.4.1",
    "popper.js": "1.15.0",
    "source-map-support": "0.5.12",
    "vue": "2.6.10",
    "vue-axios": "2.1.4",
    "vue-electron": "1.0.6",
    "vue-router": "3.0.6",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "electron": "5.0.4",
    "electron-builder": "20.44.4",
    "electron-webpack": "2.7.2",
    "electron-webpack-vue": "2.3.0",
    "node-sass": "4.12.0",
    "sass-loader": "7.1.0",
